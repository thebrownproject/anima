# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your_service_role_key_here

# OpenRouter API Configuration (for LangChain extraction)
OPENROUTER_API_KEY=sk-or-xxx
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet  # or any model available on OpenRouter (e.g., openai/gpt-4o, google/gemini-pro)

# DeepInfra API Configuration (for DeepSeek-OCR)
DEEPINFRA_API_KEY=your_deepinfra_token_here
DEEPSEEK_OCR_MODEL=deepseek-ai/DeepSeek-OCR

# Application Settings
ENVIRONMENT=development
DEBUG=False

# CORS Configuration (Frontend URL, comma-separated for multiple)
ALLOWED_ORIGINS=http://localhost:3000
